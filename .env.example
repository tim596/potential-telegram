# Environment Configuration
# Copy this file to .env and update with your actual values

# Build Environment
NODE_ENV=development

# Site Configuration
SITE_URL=https://abedderworld.com
CDN_URL=

# Analytics & Tracking (Optional)
GOOGLE_ANALYTICS_ID=
